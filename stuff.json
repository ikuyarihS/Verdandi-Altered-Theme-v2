[
    {
        "name": "Variable and parameter name",
        "scope": [
            "variable",
            "meta.definition.variable.name",
            "support.variable"
        ],
        "settings": {
            "fontStyle": "",
            "foreground": "#a1a8ae"
        }
    },
    {
        "name": "Object keys, TS grammar specific",
        "scope": [
            "meta.object-literal.key",
            "meta.object-literal.key entity.name.function"
        ],
        "settings": {
            "fontStyle": "",
            "foreground": "#a1a8ae"
        }
    },
    {
        "name": "Comment",
        "scope": [
            "comment",
            "punctuation.comment",
            "punctuation.definition.comment"
        ],
        "settings": {
            "fontStyle": "",
            "foreground": "#5b646c"
        }
    },
    {
        "name": "Operator",
        "scope": [
            "keyword.operator.logical.python"
        ],
        "settings": {
            "fontStyle": "bold",
            "foreground": "#3498db"
        }
    },
    {
        "name": "Punctuation",
        "scope": [
            "punctuation",
            "delimiter",
            "bracket",
            "brace",
            "paren",
            "delimiter.tag",
            "punctuation.tag",
