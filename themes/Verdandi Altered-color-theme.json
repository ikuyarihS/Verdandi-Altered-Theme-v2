  "tokenColors": [
    {
      "scope": ["meta.template.expression.js"],
      "settings": {
        "foreground": "#ffffff"
      }
    },
    {
      "scope": [
        "meta.definition.variable.name",
        "meta.object-literal.key",
        "meta.object-literal.key entity.name.function",
        "variable.other.property.js",
        "support.variable"
      ],
      "settings": {
        "foreground": "#a1a8ae"
      }
    },
    {
      "scope": [
        "comment",
        "punctuation.comment",
        "punctuation.definition.comment",
        "string.quoted.docstring.multi.python"
      ],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#34495E"
      }
    },
    {
      "scope": [
        "brace",
        "bracket",
        "delimiter",
        "delimiter.tag",
        "meta.brace",
        "meta.property-value punctuation.separator.key-value",
        "paren",
        "punctuation",
        "punctuation.definition.metadata.md",
        "punctuation.tag",
        "string.link.md",
        "tag.html",
        "tag.xml"
      ],
      "settings": {
        "foreground": "#7e868d"
      }
    },
    {
      "scope": [
        "beginning.punctuation",
        "entity.name.type.md",
        "entity.other.attribute-name.class.css",
        "markup.link",
        "meta.image.inline",
        "meta.link.inline",
        "meta.preprocessor",
        "meta.property-name.css",
        "punctuation.definition.heading.md",
        "string.other.link.description",
        "string.other.link.title",
        "variable.language.makefile",
        "variable.other.makefile"
      ],
      "settings": {
        "foreground": "#d6dade"
      }
    },
    {
      "scope": [
        "constant.language",
        "constant.numeric",
        "meta.property-value.color",
        "meta.property-value.numeric",
        "support.constant.property-value.color",
        "support.constant.property-value.numeric"
      ],
      "settings": {
        "foreground": "#b5cea8"
      }
    },
    {
      "scope": [
        "constant.character",
        "entity.name.tag.table",
        "entity.other.attribute-name",
        "entity.other.attribute-name.html",
        "entity.other.inherited-class",
        "keyword.control",
        "keyword.other.class.cs",
        "keyword.operator.logical.python",
        "punctuation.definition.template-expression.begin.js",
        "punctuation.definition.template-expression.end.js",
        "storage",
        "storage.type"
      ],
      "settings": {
        "foreground": "#1a5276"
      }
    },
    {
      "scope": [
        "entity.name",
        "entity.name.class",
        "entity.other.inherited-class",
        "constant.other",
        "keyword.type.cs",
        "support.class",
        "support.constant",
        "support.type"
      ],
      "settings": {
        "foreground": "#008080"
      }
    },
    {
      "scope": ["invalid", "invalid.deprecated"],
      "settings": {
        "foreground": "#b30e0e"
      }
    },
    {
      "scope": "token.info-token",
      "settings": {
        "foreground": "#6796e6"
      }
    },
    {
      "scope": "token.warn-token",
      "settings": {
        "foreground": "#cd9731"
      }
    },
    {
      "scope": "token.error-token",
      "settings": {
        "foreground": "#f44747"
      }
    },
    {
      "scope": "token.debug-token",
      "settings": {
        "foreground": "#b267e6"
      }
    },
    {
      "scope": ["string", "meta.structure.dictionary.json string.quoted.double.json"],
      "settings": {
        "foreground": "#E9967A"
      }
    },
    {
      "scope": [
        "entity.name.variable",
        "variable.language.this.js",
        "variable.language.special.self.python",
        "variable.parameter.js",
        "variable.parameter.function.language.python",
        "variable.parameter.function-call.python",
        "support.function.lua"
      ],
      "settings": {
        "foreground": "#E74C3C"
      }
    },
    {
      "scope": [
        "entity.name.function",
        "meta.function-call.generic.python",
        "meta.function.python",
        "punctuation.definition.dict.begin.python",
        "punctuation.definition.dict.end.python",
        "punctuation.definition.list.begin.python",
        "punctuation.definition.list.end.python",
        "support.function.builtin.python",
        "support.type.property-name",
        "support.function.any-method.lua"
      ],
      "settings": {
        "foreground": "#3498db"
      }
    },
    {
      "scope": "constant.regexp",
      "settings": {
        "foreground": "#646695"
      }
    },
    {
      "scope": "string.regexp",
      "settings": {
        "foreground": "#d16969"
      }
    },
    {
      "name": "Regular expression groups",
      "scope": [
        "punctuation.definition.group.regexp",
        "punctuation.definition.group.assertion.regexp",
        "punctuation.definition.character-class.regexp",
        "punctuation.character.set.begin.regexp",
        "punctuation.character.set.end.regexp",
        "keyword.operator.negation.regexp",
        "support.other.parenthesis.regexp"
      ],
      "settings": {
        "foreground": "#CE9178"
      }
    },
    {
      "scope": [
        "constant.character.character-class.regexp",
        "constant.other.character-class.set.regexp",
        "constant.other.character-class.regexp",
        "constant.character.set.regexp"
      ],
      "settings": {
        "foreground": "#d16969"
      }
    },
    {
      "scope": ["keyword.operator.or.regexp", "keyword.control.anchor.regexp"],
      "settings": {
        "foreground": "#DCDCAA"
      }
    },
    {
      "scope": "keyword.operator.quantifier.regexp",
      "settings": {
        "foreground": "#d7ba7d"
      }
    }
  ]
}
